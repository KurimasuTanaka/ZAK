
<Button onclick="SwapButtonIsPressed">
    <Icon Name="IconName.ArrowDownUp" />
</Button>

@code {
    [Parameter] public int applicationId { get; set; } = 0;
    [Parameter] public EventCallback<int> swapButtonIsPressed { get; set; }

    private async Task SwapButtonIsPressed()
    {
        await swapButtonIsPressed.InvokeAsync();
    }
}
