{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": "Launch .NET Application",
      "program": "${workspaceFolder}/${input:selectProject}/bin/Debug/net9.0/${workspaceFolderBasename}.dll",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build"
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "selectProject",
      "description": "Select the project to debug",
      "options": [
        "ZAK",
        "ZAK.ApplicationsLoader",
        "ZAK.ApplicationsScrapper",
        "ZAK.Da",
        "ZAK.DAO",
        "ZAK.Db",
        "ZAK.Enums",
        "ZAK.GeoDataFetcher",
        "ZAK.MapRouter"
      ]
    }
  ]
}