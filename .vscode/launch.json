{
  "version": "0.2.0",
  "workspaceFolder": " D:\\C#_Projects\\ZAK\\ZAK3\\ZAK\\ZAK",
  "configurations": [
    {
      "name": "Launch ASP.NET Core Blazor Server",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ZAK/ZAK/bin/Debug/net9.0/ZAK.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}"
      }
    }
  ]
}
